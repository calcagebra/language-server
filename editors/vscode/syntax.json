{
  "scopeName": "source.cal",
  "patterns": [
    { "include": "#lineComment" },
    { "include": "#preprocessorStart" }
  ],
  "repository": {
    "lineComment": {
      "name": "comment.line.cal",
      "begin": "//"
    },
    "preprocessorStart": {
      "name": "keyword.control.directive.cal",
      "match": "^#(end|else|then|if)"
    }
  }
}
